#include <stdio.h>
#include <iostream>

int main()
{
    std::string gwiazdka = "";
    std::string sumaGwiazd = "";
    std::cout<<"Podaj wymiar";
    int x ;
    std::cin>>x;
    
    for(int i = 0;i<x;i++){
        
        gwiazdka = gwiazdka +"*";
        sumaGwiazd +=gwiazdka + "\n";
    }
    
    std::cout << sumaGwiazd << std::endl;
    

    return 0;
}
