#include <stdio.h>
#include <iostream>

int main()
{
    int a,b;
    
    std::cout << "Podaj a" << std::endl;
    std::cin>>a;
    std::cout<<""<<std::endl;
    
    std::cout << "Podaj b" << std::endl;
    std::cin>>b;
    std::cout<<""<<std::endl;
    
    if(a>b){
        std::cout<<a;
        std::cout<<">";
        std::cout<<b<<std::endl;
    }else if(a<b){
        std::cout<<a;
        std::cout<<"<";
        std::cout<<b<<std::endl;
    }else if(a==b){
        std::cout<<a;
        std::cout<<"=";
        std::cout<<b<<std::endl;
    }

    return 0;
}
