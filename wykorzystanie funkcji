#include <iostream>
#include <string>

using namespace std;

    auto askUser(std::string argumentFunkcji)-> int
    {
        std::cout<< argumentFunkcji << "int: ";
        auto zmiennaLokalna = std::string{};
        std::getline(std::cin,zmiennaLokalna);
        return std::stoi(zmiennaLokalna);
    }
    

    void odejmowanie(int a, int b){
        int suma = 0;
        suma = a - b;
        cout<<suma;
    }


int main()
{

  int a = askUser("a=");
  int b = askUser("B=");
    odejmowanie(a,b);

    return 0;
}
