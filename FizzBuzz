#include <iostream>

using namespace std;

int main()
{
    int x ;
    std::cout<<"Podaj liczbe: ";
    std::cin>>x;
    if(x<0){
        std::cout<<"ERROR";
        return 0 ;
    }
    
    if(x%3==0 && x%5==0){
        std::cout<<"FizzBuzz";
    }else if(x%5==0){
        std::cout<<"Buzz";
    }else if(x%3==0){
        std::cout<<"Fizz";
    }
}    
 
    
    
