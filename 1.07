#include <stdio.h>
#include <iostream>

int main()
{
    int x;
    std::cout<<"Wpisz cyfre: "<<std::endl;
    std::cin >> x;
    
    if(x>0){
        std::cout << "Dodatnia" << std::endl;
    }else if(x==0){
        std::cout<<"ZERO";
    }else if(x<0){
        std::cout<<"Ujemna";
    }

    return 0;
}

